<?xml version="1.0" encoding="utf-8"?>
<com.bingo.percentsamples.PercentLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.bingo.percentsamples.PercentLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:layout_marginTopPercent="10%"
        android:orientation="vertical">
        <ImageView
            android:id="@id/audio_call_photo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"/>
        <TextView
            android:id="@id/audio_call_name"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_gravity="center"/>
        <com.bingo.percentsamples.PercentLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:orientation="horizontal">
            <ImageView
                android:id="@id/audio_call_type"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>

         </com.bingo.percentsamples.PercentLinearLayout>
     </com.bingo.percentsamples.PercentLinearLayout>

    <com.bingo.percentsamples.PercentLinearLayout
        android:id="@id/audio_call_number_layout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_heightPercent="5%"
        app:layout_widthPercent="35%"
        app:layout_marginTopPercent="4%"
        android:layout_gravity="center">
        <EditText
            android:id="@id/audio_call_number_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="42px"
            android:numeric="integer"
            android:digits="0123456789*#"
            android:singleLine="true"
            android:background="@drawable/audio_call_number_bg"/>
     </com.bingo.percentsamples.PercentLinearLayout>

    <android.support.percent.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="29%"
        app:layout_marginTopPercent="1%">
    <com.bingo.percentsamples.PercentLinearLayout
            android:id="@id/audio_call_operation_left_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            app:layout_widthPercent="13%"
            app:layout_marginLeftPercent="18%"
            android:visibility="visible"
            android:background="@drawable/audio_call_digit_keyboard">
            <GridView
                android:id="@+id/audio_call_digit_keyboard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15px"
                android:layout_marginLeft="15px"
                android:layout_marginTop="18px"
                android:layout_marginBottom="18px"
                android:verticalSpacing="10px"
                android:horizontalSpacing="10px"
                android:numColumns="3">
            </GridView>
         </com.bingo.percentsamples.PercentLinearLayout>

        <com.bingo.percentsamples.PercentLinearLayout
            android:id="@id/audio_call_operation_center_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_widthPercent="35%"
            android:layout_centerHorizontal="true"
            android:background="@drawable/audio_call_operation_center_layout">
            <com.bingo.percentsamples.PercentLinearLayout
                android:id="@id/audio_call_operation_center_first_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_heightPercent="32%"
                android:orientation="horizontal">

                <com.bingo.percentsamples.PercentLinearLayout
                    android:id="@id/audio_call_dial_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="33%"
                    android:gravity="center"
                    android:orientation="vertical" >
                    <ImageView
                        android:id="@id/audio_call_dial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_dial_hide"/>
                    <TextView
                        android:id="@id/audio_call_dial_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_dial_str"/>
                </com.bingo.percentsamples.PercentLinearLayout>
                <ImageView
                    android:id="@id/audio_call_vertical_line"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/audio_call_vertical_line"/>
                <com.bingo.percentsamples.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="33%"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@id/audio_call_switch_video_call"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_switch_video_call"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_switch_video_call_str"/>
                </com.bingo.percentsamples.PercentLinearLayout>
                <ImageView
                    android:id="@id/audio_call_vertical_line"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/audio_call_vertical_line"/>
                <com.bingo.percentsamples.PercentLinearLayout
                    android:id="@id/audio_call_more_option_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    app:layout_widthPercent="33%"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@id/audio_call_more_option"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_more_option_up"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_more_option_str"/>
                 </com.bingo.percentsamples.PercentLinearLayout>
            </com.bingo.percentsamples.PercentLinearLayout>

            <ImageView
                android:id="@id/audio_call_horizontal_line_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_marginTopPercent="3%"
                app:layout_marginBottomPercent="3%"
                android:visibility="gone"
                android:src="@drawable/audio_call_horizontal_line"/>

            <com.bingo.percentsamples.PercentLinearLayout
                android:id="@id/audio_call_operation_center_second_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_heightPercent="32%">
                <com.bingo.percentsamples.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="33%"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@id/audio_call_record"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_record"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_record_str"/>
                </com.bingo.percentsamples.PercentLinearLayout>

                <ImageView
                    android:id="@id/audio_call_vertical_line"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/audio_call_vertical_line"/>

                <com.bingo.percentsamples.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="33%"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@id/audio_call_contact"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_contact"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_contact_str"/>
                </com.bingo.percentsamples.PercentLinearLayout>

                <ImageView
                    android:id="@id/audio_call_vertical_line"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/audio_call_vertical_line"/>

                <com.bingo.percentsamples.PercentLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    app:layout_widthPercent="33%">
                    <ImageView
                        android:id="@id/audio_call_add_talk"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_marginTopPercent="30%"
                        android:src="@drawable/audio_call_add_talk"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audio_call_add_talk_str"/>

                 </com.bingo.percentsamples.PercentLinearLayout>

            </com.bingo.percentsamples.PercentLinearLayout>

            <ImageView
                android:id="@id/audio_call_horizontal_line_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_marginTopPercent="3%"
                app:layout_marginBottomPercent="3%"
                android:src="@drawable/audio_call_horizontal_line"/>

            <TextView
                android:id="@id/audio_call_hang_up"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/audio_call_hang_up_str"
                android:background="@drawable/audio_call_hang_up"
                android:gravity="center_vertical|center_horizontal"
                app:layout_heightPercent="18%"
                app:layout_widthPercent="94%"
                app:layout_marginPercent="3%"/>
        </com.bingo.percentsamples.PercentLinearLayout>

    </android.support.percent.PercentRelativeLayout>
</com.bingo.percentsamples.PercentLinearLayout>